# overwrite these to send straight to remote (chained) tmux
source ~/.tmux.conf
bind-key -n M-0 send-key M-0
bind-key -n M-1 send-key M-1
bind-key -n M-2 send-key M-2
bind-key -n M-3 send-key M-3
bind-key -n M-4 send-key M-4
bind-key -n M-5 send-key M-5
bind-key -n M-6 send-key M-6
bind-key -n M-7 send-key M-7
bind-key -n M-8 send-key M-8
bind-key -n M-9 send-key M-9
bind-key -n M-- send-key M--
bind-key -n M-, send-key M-,
bind-key -n M-j send-key M-j
bind-key -n M-k send-key M-k
